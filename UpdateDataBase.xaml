<Window x:Class="SaveBackup.UpdateDataBase"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		Title="UpdateDataBase" Height="Auto" Width="Auto" Topmost="False" mc:Ignorable="d" xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" d:DesignWidth="176" SizeToContent="WidthAndHeight">
	<DockPanel Height="Auto" Name="dockPanel1" Width="Auto">
		<ProgressBar DockPanel.Dock="Top" Height="23" Name="downloadProgress" Width="Auto" />
		<Label DockPanel.Dock="Top" Content="Label" Height="28" Name="label1" />
		<Button DockPanel.Dock="Left" Content="Button" Height="23" Name="Update" Width="75" Click="UpdateClick" />
		<Button DockPanel.Dock="Left" Content="Button" Height="23" Name="CancelDownloading" Width="75" Visibility="Collapsed" />
	</DockPanel>
</Window>
